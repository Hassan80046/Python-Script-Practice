    "def myfunc(name):\n",
    "    c=0\n",
    "    word=[]\n",
    "    for alphabet in name:\n",
    "        c=c+1\n",
    "        if c%2==0:\n",
    "            word.append(alphabet.upper())\n",
    "        else:\n",
    "            word.append(alphabet.lower())\n",
    "    string1=''.join(word)\n",
    "    print(string1)\n",
    "    return string1"
